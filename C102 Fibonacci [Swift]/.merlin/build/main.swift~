let n = 7
// DO NOT ALTER THE TEXT BETWEEN THESE LINES =========={M{E{R{L{I{N{1}N}I}L}R}E}M}=====================================
// ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧  ⇧


// Add your code below:

func fibonacci(int n: Int) -> Int {
    print("Calculating fibonacci(\(n))...")



    if n > 2 {


        let prevsum = fibonacci(int: n-2) + fibonacci(int: n-1)
        print("Non-terminal case, returning \(prevsum).")
        return prevsum
    } else {
        print("Terminal case, returning 1.")
        return 1
    }
}
print("The final result is: \(fibonacci(int: n)).")
